{
  "name": "JSONUpdateWatcher",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/logo-round-128.png",
    "icons": {
      "16": "images/logo-round-16.png",
      "48": "images/logo-round-48.png",
      "128": "images/logo-round-128.png"
    },
    "default_title": "JSON Update Watcher",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/jquery-2.1.3.js", "background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/Devex/front-end/pull/*", "https://github.com/Devex/grape-api/pull/*"],
      "js": ["js/jquery-2.1.3.js", "content.js"]
    }
  ]
}
